{
  "name": "storycomputer",
  "version": "0.0.1",
  "description": "Story Computer",
  "keywords": [
    "storycomputer"
  ],
  "homepage": "https://github.com/storycomputer/storycomputer",
  "bugs": "https://github.com/storycomputer/storycomputer/issues",
  "license": "Apache-2.0",
  "author": "Stanislav Senotrusov",
  "main": "./dist/main.js",
  "bin": {},
  "repository": "storycomputer/storycomputer",
  "scripts": {
    "build": "tsc",
    "make": "electron-forge make",
    "package": "electron-forge package",
    "start": "npm run build && electron-forge start",
    "test": "cross-env NODE_ENV=test mocha",
    "watch": "tsc --watch"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "sqlite3": "5.0.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@types/mocha": "^8.2.2",
    "@types/sqlite3": "^3.1.7",
    "cross-env": "^7.0.3",
    "electron": "^11.0.3",
    "mocha": "^8.4.0",
    "spectron": "^13.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "storycomputer"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
